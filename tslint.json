{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-empty-interface": false,
    "ordered-imports": false,
    "member-access": false,
    "strict-boolean-expressions": false
  },
  "jsRules": {
    "jsx-boolean": false
  }
}
